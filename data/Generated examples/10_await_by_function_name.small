// Example 10: Await by function name after spawn without handle (if supported)
function increment() { a = a + 1; return a; }

function main() {
  a = 0;
  spawn increment();       // no explicit handle
  a = a + 1;               // Parent R/W(a) => race expected vs increment
  await increment;         // await by "name" (convention)
  return a;
}
